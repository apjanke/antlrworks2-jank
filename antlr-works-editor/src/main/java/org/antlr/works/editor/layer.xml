<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-antlr3">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>

                <folder name="Preferences">
                    <folder name="Defaults">
                        <file name="org-antlr-works-editor-grammar-preferences.xml" url="grammar/resources/DefaultPreferences.xml" />
                    </folder>
                </folder>

                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="grammar/highlighter/FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>

                <folder name="FoldManager">
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
                </folder>

                <folder name="Popup">
                    <folder name="goto">
                        <attr name="position" intvalue="500"/>
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>
                        <file name="goto-declaration">
                            <attr name="position" intvalue="200"/>
                        </file>
                    </folder>

                    <folder name="refactor">
                        <attr name="position" intvalue="600"/>
                        <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>
                    </folder>

                    <file name="SeparatorBeforeCut.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="1960"/>
                    </file>

                    <file name="SeparatorAfterPaste.instance">
                        <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                        <attr name="position" intvalue="4100"/>
                    </file>
                </folder>

                <folder name="Toolbars">
                    <folder name="Default">
                        <file name="Separator-before-comment.instance">
                            <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                            <attr name="position" intvalue="19100"/>
                        </file>
                        <file name="comment">
                            <attr name="position" intvalue="19200"/>
                        </file>
                        <file name="uncomment">
                            <attr name="position" intvalue="19300"/>
                        </file>
                        <file name="separator-before-compatibility-mode.instance">
                            <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                            <attr name="position" intvalue="20100"/>
                        </file>
                        <file name="toggle-legacy-mode">
                            <attr name="position" intvalue="20200"/>
                        </file>
                    </folder>
                </folder>

                <file name="AutoIndenter.instance">
                    <attr name="instanceOf" stringvalue="org.netbeans.spi.editor.typinghooks.TypedTextInterceptor$Factory"/>
                    <attr name="instanceCreate"
                          methodvalue="org.netbeans.modules.editor.indent.spi.support.AutomatedIndenting.createHotCharsIndenter"/>
                    <attr name="regex1" stringvalue="\s*\{"/>
                    <attr name="regex2" stringvalue="\s*\}"/>
                    <attr name="regex3" stringvalue="\s*:"/>
                    <attr name="regex4" stringvalue="\s*\|"/>
                    <attr name="regex5" stringvalue="\s*;"/>
                    <attr name="regex6" stringvalue="\s*\)"/>
					<!-- Target-specific action headers. -->
                    <attr name="regex7" stringvalue="\s*\@"/>
                </file>
<!--                <folder name="CompletionProviders">
                    <file name="org-antlr-works-editor-grammar-GrammarCompletionProvider.instance"/>
                    <file name="org-netbeans-modules-java-editor-javadoc-JavadocCompletionProvider.instance"/>
                </folder>-->

                <!--<folder name="UpToDateStatusProvider">
                    <file name="org-netbeans-modules-java-editor-semantic-OccurrencesMarkProviderCreator.instance">
                    </file>
                </folder>-->

                <!--<folder name="GlyphGutterActions">
                    <file name="org-netbeans-modules-java-editor-overridden-IsOverriddenAnnotationAction.instance">
                        <attr name="position" intvalue="100"/>
                    </file>
                </folder>-->
            </folder>
            <folder name="x-antlr3-ldbg">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>

                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="grammar/debugger/FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>

                <folder name="FoldManager">
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
                </folder>

                <!--<folder name="Toolbars">
                    <folder name="Default">
                        <file name="separator-before-compatibility-mode.instance">
                            <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                            <attr name="position" intvalue="20100"/>
                        </file>
                        <file name="toggle-legacy-mode">
                            <attr name="position" intvalue="20200"/>
                        </file>
                    </folder>
                </folder>-->
            </folder>
            <folder name="x-stringtemplate4">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>

                <folder name="Preferences">
                    <folder name="Defaults">
                        <file name="org-antlr-works-editor-st4-preferences.xml" url="st4/resources/DefaultPreferences.xml" />
                    </folder>
                </folder>

                <folder name="FontsColors">
                    <folder name="NetBeans">
                        <folder name="Defaults">
                            <file name="FontAndColors.xml" url="st4/highlighter/FontAndColors.xml">
                                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.antlr.works.editor.Bundle"/>
                            </file>
                        </folder>
                    </folder>
                </folder>

                <folder name="FoldManager">
                    <file name="org-netbeans-editor-CustomFoldManager$Factory.instance"/>
                </folder>

                <folder name="Toolbars">
                    <folder name="Default">
                        <file name="Separator-before-comment.instance">
                            <attr name="instanceClass" stringvalue="javax.swing.JSeparator"/>
                            <attr name="position" intvalue="19100"/>
                        </file>
                        <file name="comment">
                            <attr name="position" intvalue="19200"/>
                        </file>
                        <file name="uncomment">
                            <attr name="position" intvalue="19300"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="OptionsDialog">
        <folder name="PreviewExamples">
            <folder name="text">
                <file name="x-antlr3" url="grammar/GrammarTemplate.g"/>
                <file name="x-stringtemplate4" url="st4/StringTemplateFileTypeTemplate.stg"/>
